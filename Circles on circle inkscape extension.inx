            <param name="stroke-w-9" type="float" precision="3" min="0.001" max="1000.000" _gui-text="">0.500</param>
        </hbox>
        <hbox>
            <label>10</label>
            <param name="include-10" type="boolean" _gui-text="">false</param>
            <param name="diameter-10" type="float" precision="3" min="0.001" max="10000.000" _gui-text="">2</param>
            <param name="stroke-w-10" type="float" precision="3" min="0.001" max="1000.000" _gui-text="">0.500</param>
        </hbox>
    </page>
</param>
<effect>
    <object-type>all</object-type>
    <effects-menu>
        <submenu _name="KaTaai"/>
    </effects-menu>
</effect>
<script>
    <command reldir="extensions" interpreter="python">Circles_on_circle_gui.py</command>
</script>
</inkscape-extension>